{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "DesignPattern",
  "type": "object",
  "required": [
    "id",
    "title",
    "context",
    "problem",
    "forces",
    "solution"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "context": {
      "type": "string"
    },
    "problem": {
      "type": "string"
    },
    "forces": {
      "type": "string"
    },
    "solution": {
      "type": "string"
    },
    "generated": {
      "type": "string",
      "format": "date-time"
    }
  }
}